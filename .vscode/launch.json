  "version": "0.2.0",
  "configurations": [
    {
      "name": "Py: Attach to PID",
      "type": "python",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "justMyCode": true
    },
    {
      "name": "Py: module disco_snake",
      "type": "python",
      "request": "launch",
      "module": "disco_snake",
      "args": ["start", "--debug"],
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
